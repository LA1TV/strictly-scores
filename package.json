{
  "name": "strictly-scores",
  "version": "1.0.0",
  "description": "CG, Judging, Scores plus audience votes",
  "scripts": {
    "start": "NODE_ENV=production babel-node server.js",
    "test": "mocha test/**/*.test.js --reporter spec --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LA1TV/strictly-scores.git"
  },
  "keywords": [
    "express",
    "angularjs",
    "nodejs"
  ],
  "author": "Luke Moscrop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LA1TV/strictly-scores/issues"
  },
  "homepage": "https://github.com/LA1TV/strictly-scores#readme",
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "babel-register": "6.3.13",
    "mocha": "^2.5.3",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "express": "^4.14.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    },
    "ava": {
      "files": [
        "test/*.js"
      ],
      "require": [
        "babel-register"
      ],
      "babel": "inherit"
    }
  }
}
